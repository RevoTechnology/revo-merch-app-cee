<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid android:color="@color/dark_orange"/>
            <stroke android:width="2dp"
                android:color="@color/white"/>
            <size
                android:width="64dp"
                android:height="64dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_person"
        android:top="4dp"
        android:left="4dp"
        android:right="4dp"
        android:bottom="4dp"/>
</layer-list>

